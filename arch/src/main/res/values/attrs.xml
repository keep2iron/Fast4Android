<resources>
  <declare-styleable name="SwipeBackLayout">
    <attr name="edge_flag">
      <enum name="left" value="0"/>
      <enum name="right" value="1"/>
      <enum name="bottom" value="2"/>
      <enum name="all" value="3"/>
    </attr>
    <attr format="reference" name="shadow_left"/>
    <attr format="reference" name="shadow_right"/>
    <attr format="reference" name="shadow_bottom"/>
  </declare-styleable>

  <attr format="reference" name="SwipeBackLayoutStyle"/>
</resources>