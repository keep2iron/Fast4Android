  <!--**************** FastPullRefreshLayout ******************-->
  <declare-styleable name="FastPullRefreshLayout">
    <!-- RefreshView的初始offset -->
    <attr format="dimension" name="fast_refresh_init_offset"/>
    <!-- 刷新时RefreshView的offset -->
    <attr format="dimension" name="fast_refresh_end_offset"/>
    <!-- TargetView(ListView或者ScrollView等)的初始位置 -->
    <attr format="dimension" name="fast_target_init_offset"/>
    <!-- 刷新时TargetView(ListView或者ScrollView等)的位置-->
    <attr format="dimension" name="fast_target_refresh_offset"/>
    <!-- 是否自动根据RefreshView的高度计算RefreshView的初始位置-->
    <attr format="boolean" name="fast_auto_calculate_refresh_init_offset"/>
    <!-- 是否自动根据TargetView在刷新时的位置计算RefreshView的结束位置-->
    <attr format="boolean" name="fast_auto_calculate_refresh_end_offset"/>
    <!-- TargetView的刷新位置与RefreshView高度相等-->
    <attr format="boolean" name="fast_equal_target_refresh_offset_to_refresh_view_height"/>
  </declare-styleable>
  <attr format="reference" name="FastPullRefreshLayoutStyle"/>

